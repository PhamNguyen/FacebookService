<UserControl x:Class="TesttingFacebookSDKInLibrary.WindowsPhoneControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             FontFamily="{StaticResource PhoneFontFamilyNormal}"
             FontSize="{StaticResource PhoneFontSizeNormal}"
             Foreground="{StaticResource PhoneForegroundBrush}"
             d:DesignHeight="480"
             d:DesignWidth="480">
    <StackPanel x:Name="LayoutRoot"
                Background="{StaticResource PhoneChromeBrush}">
        <Button Content="Login Facebook from Library"
                VerticalAlignment="Center"
                HorizontalAlignment="Center"
                Tap="Button_Tap" Margin="12"/>
        <Button Content="Post Feed Facebook from Library"
                VerticalAlignment="Center"
                HorizontalAlignment="Center"
                Tap="ButtonPostFeed_Tap"
                Margin="12" />
        <Button Content="Logout Facebook from Library"
                VerticalAlignment="Center"
                HorizontalAlignment="Center"
                Tap="ButtonLogout_Tap"
                Margin="12" />
    </StackPanel>
</UserControl>